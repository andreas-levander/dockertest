{
  "$schema": "https://www.krakend.io/schema/v3.json",
  "version": 3,
  "name": "KrakenD - API Gateway test",
  "timeout": "3000ms",
  "cache_ttl": "300s",
  "output_encoding": "json",
  "endpoints": [
    {
      "endpoint": "/v1/test",
      "method": "GET",
      "backend": [
        {
          "url_pattern": "/api/test",
          "sd": "static",
          "method": "GET",
          "host": [
            "http://130.232.178.29:3000"
          ],
          "disable_host_sanitize": false
        }
      ],
      "input_headers": [
        "*"
      ]
    }, {
    "endpoint": "/v1/prot",
    "method": "GET",
    "extra_config": {
      "auth/validator": {
          "alg": "PS256",
          "jwk_url": "http://130.232.178.29:4000/verify",
          "cache": true,
          "disable_jwk_security": true
      }
    },
    "backend": [
      {
        "url_pattern": "/api/prot",
        "sd": "static",
        "method": "GET",
        "host": [
          "http://130.232.178.29:3000"
        ],
        "disable_host_sanitize": false
      }
    ],
    "input_headers": [
      "*"
    ]
  }
  ],
  "extra_config": {
    "security/cors": {
      "allow_origins": [
        "*"
      ],
      "expose_headers": [
        "Content-Length"
      ],
      "max_age": "12h",
      "allow_methods": [
        "GET",
        "HEAD",
        "POST"
      ],
      "allow_headers": [
        "*"
      ]
    }
  }
}